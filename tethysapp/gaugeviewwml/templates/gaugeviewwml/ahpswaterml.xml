<?xml version="1.0" encoding="utf-8" ?>
<timeSeriesResponse xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.cuahsi.org/waterML/1.1/">
	<queryInfo><creationTime>{{metadata.ReqTime}}</creationTime><criteria MethodCalled="GetValues"><parameter name="site" value="user defined" /><parameter name="variable" value="{{metadata.UnitAbbv}}" /></criteria></queryInfo>
	<timeSeries>
		<sourceInfo xsi:type="SiteInfoType"><siteName>{{ metadata.SiteName }}</siteName><siteCode network="AHPS">{{ metadata.GaugeID }}</siteCode><geoLocation><geogLocation xsi:type="LatLonPointType"><latitude>{{ metadata.Lat }}</latitude><longitude>{{ metadata.Long }}</longitude></geogLocation></geoLocation></sourceInfo>
		<variable><variableCode vocabulary="AHPS" default="true" variableID="{{metadata.VarCode}}" >{{metadata.VarName}}</variableCode><variableName>{{metadata.VarName}}</variableName><valueType>Derived Value</valueType><dataType>Continuous</dataType><generalCategory>Hydrology</generalCategory><sampleMedium>Surface Water</sampleMedium><unit><unitName>{{metadata.UnitName}}</unitName><unitType>{{metadata.VarName}}</unitType><unitAbbreviation>{{metadata.UnitAbbv}}</unitAbbreviation><unitCode>{{metadata.VarCode}}</unitCode></unit><noDataValue>-9999</noDataValue><timeScale><unit><unitName>Minute</unitName><unitType>Time</unitType><unitAbbreviation>m</unitAbbreviation></unit></timeScale><speciation>Not applicable</speciation></variable>

		<values>
        {% for tvp in time_series %}
			<value censorCode="nc" dateTime="{{tvp.0}}" timeOffset="{{tvp.1}}" dateTimeUTC="{{tvp.2}}"  methodCode="1"  sourceCode="1"  qualityControlLevelCode="{{tvp.5}}" >{{tvp.4}}</value>
	    {% endfor %}
		<qualityControlLevel qualityControlLevelID="1"><qualityControlLevelCode>1</qualityControlLevelCode><definition>Raw data</definition><explanation>The data was not manually quality controlled. Errors may exist in the data.</explanation></qualityControlLevel><qualityControlLevel qualityControlLevelID="3"><qualityControlLevelCode>3</qualityControlLevelCode><definition>Derived products</definition><explanation>This data is derived forecast data, and is not meant to represent actual conditions.</explanation></qualityControlLevel><method methodID="1"><methodCode>1</methodCode><methodDescription>The original data is from the NOAA NWS AHPS web service. It was extracted by Gauge Viewer WaterML App through the NOAA NWS AHPS web service.</methodDescription><methodLink></methodLink></method><source sourceID="1"><sourceCode>1</sourceCode><organization>NOAA NWS AHPS</organization><sourceDescription>NOAA NWS AHPS and BYU</sourceDescription><contactInformation><contactName>Bryce Anderson</contactName><typeOfContact>main</typeOfContact><email>bwanderson@users.noreply.github.com</email><address xsi:type="xsd:string">Clyde Building, Provo, Utah, 84604</address></contactInformation><sourceLink>http://tethys.byu.edu/apps/gaugeviewwml/</sourceLink><citation>NOAA NWS AHPS observed and forecasted data for the selected gauge extracted using the Gauge Viewer WaterML application. Data source: http://water.weather.gov/ahps/</citation></source><censorCode><censorCode>nc</censorCode><censorCodeDescription>not censored</censorCodeDescription></censorCode></values></timeSeries></timeSeriesResponse>